{
    "compilerOptions": {
        "target": "es5",
        "module": "amd",
        "outFile": "app.js",
        "noImplicitAny": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "noEmitOnError": true,
        "experimentalDecorators": true,
        "noUnusedParameters": false
    },
    "include": [ "app.ts" ],
    "exclude": [
        "node_modules"
    ]
}